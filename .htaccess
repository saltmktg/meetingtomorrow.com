#Begin rewrites
Options +FollowSymLinks
RewriteEngine on

#Force non-www:
RewriteCond %{HTTP_HOST} ^www\.meetingtomorrow\.com [NC]
RewriteRule ^(.*)$ http://meetingtomorrow.com/$1 [L,R=301]

#Rewrite stg site to main site
RewriteCond %{HTTP_HOST} ^stg.meetingtomorrow\.com$
RewriteRule (.*)$ http://meetingtomorrow.com/$1 [R=301,L]

#Rewrite mtstagingsite.com site to meetingtomorrow.com
RewriteCond %{HTTP_HOST} ^mtstagingsite\.com$ [OR]
RewriteCond %{HTTP_HOST} ^www\.mtstagingsite\.com$
RewriteRule (.*)$ http://meetingtomorrow.com/$1 [R=301,L]

#Rewrite anything in /category/ directory to root directory
RewriteRule ^category/(.*)$ /$1 [R=301,L]

#Rewrite anything in /cms-category/ directory to root directory
RewriteRule ^cms-category/(.*)$ /$1 [R=301,L]

# Force HTTPS
RewriteEngine On
RewriteCond %{HTTPS} off
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]


